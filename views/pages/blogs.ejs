<%- include('../partials/header.ejs') %>
<nav class="navbar">
  <h1>
    <a href="/dashboard">Dashboard | <%= user.name %></a>
  </h1>
  <ul>
    <li><a href="/logout">Log out</a></li>
  </ul>
</nav>
<div class="container">
  <main class="content">
    <% if(blogs.length > 0) { %>
    <h2>All blogs</h2>
    <hr />
    <div class="display-blogs">
      <% blogs.forEach(blog => { %>
      <a href="#"><%= blog.title %></a>
      <% }) %>
    </div>
    <% } else { %>
    <h2>No blogs</h2>
    <% } %>
  </main>
</div>
<%- include('../partials/footer.ejs') %>
